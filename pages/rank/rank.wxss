  font-size: 30rpx;
  color: #333;
}
.last{
height: 95rpx;
line-height: 95rpx;
background-color: #f6cd66;
padding: 0 30rpx;
margin-top: 10rpx;
border-bottom-left-radius:10rpx;
border-bottom-right-radius:10rpx;
}
.last .popularity{
  display: block;
  height: 95rpx;
  line-height: 95rpx;
  font-size: 24rpx;
  color: #999; 
  margin-right: 20rpx;
}
.last .user{
  width: 100%;
