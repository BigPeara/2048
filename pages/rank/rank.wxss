  page{
    width: 100%;
    height: 100%;
  }
  .rankBox{
   width: 100%;
   height: 100%;
   background-color: #2a2338;
   overflow: hidden;
}
.rank{
  display: block;
  width: 690rpx;
  margin: 150rpx 30rpx 90rpx 30rpx;
}
.rankhide{
  display: none;
  width: 690rpx;
  margin: 150rpx 30rpx 90rpx 30rpx;
}
.rank .closeBox{
  width: 690rpx;
  height: 50rpx;
  overflow: hidden;
}
.rank .closeImg{
  width: 50rpx;
  height: 50rpx;
  float: right;
  display: block;
}
.rank .rankInfo{
  width: 690rpx;
  height: 720rpx;
  background-color: #ffde94;
  border-radius:10rpx;
  -webkit-border-radius:10rpx;
  margin-top: 10rpx;
  position: relative;
}
.titleList{
  width: 690rpx;
  height: 75rpx;
  display: flex;
  display: -webkit-flex;
  display: -webkit-box; 
}
.title{
  display: block;
  width: 50%;
  line-height: 75rpx;
  text-align: center; 
}
.titleList text:nth-child(1){
 border-top-left-radius:10rpx;
}
.titleList text:nth-child(2){
 border-top-right-radius:10rpx;

}
.ranklist::-webkit-scrollbar {
  display: none;
}
.sel{
  display: block;
  width: 50%;
  line-height: 75rpx;
  text-align: center;
  background: -webkit-linear-gradient(#f6d267, #f5c266); 
  background: -o-linear-gradient(#f6d267, #f5c266); 
  background: -moz-linear-gradient(#f6d267, #f5c266);
  background: linear-gradient(#f6d267, #f5c266);
}
.show{
  width: 690rpx;
  position: relative;
  display: block;
}
.hidden{
  width: 690rpx;
  position: relative;
  display: none
}
.update{
  margin-top: 30rpx;
  text-align: center;
  display: block;
  font-size: 24rpx;
  color: #333;
  font-weight: 600;
}
.ranklist{
  width: 630rpx;
  height: 440rpx;
  margin-left: 30rpx;
  margin-top: 40rpx;
  overflow-y: scroll;
} 
.user{
  width: 100%;
  height: 60rpx;
  margin-bottom: 30rpx;
  display: flex;
  display: -webkit-flex;
  display: -webkit-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between; 
}
.uesrinfo{
  width: 70%;
  height: 60rpx;
  display: flex;
  display: -webkit-flex;
  display: -webkit-box;
}
.sort{
  display: block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  -webkit-border-radius:20rpx;
  text-align: center;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #fff; 
  margin-right: 30rpx;
}
.one{
   background-color: #facc3f;
}
.two{
background-color: #bebebe;
}
.three{
background-color: #fcb592;
}
.four{
 color: #999;
 text-shadow:1px 1px #bca486,1px 1px #bca486,1px 1px #bca486,1px 1px #bca486;
}
.five{
color: #999;
text-shadow:1px 1px #bca486,1px 1px #bca486,1px 1px #bca486,1px 1px #bca486;
}
.headImg{
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  -webkit-border-radius:30rpx;
  background-color: gray;
}
.nickname{
  width: 35%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
  color: #333;
  display: block;
  line-height: 60rpx;
  margin-left: 20rpx;
}
.score{
  line-height: 60rpx;
