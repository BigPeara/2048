.start-container{background-color: #2a2338;}
.start-conent{margin: 0 28rpx;padding-top: 158rpx;}
.start-conent .start-player{position: relative;background: linear-gradient(#f6d267,#f5c666,#f6c666);color: #333;text-align: center;border-top-left-radius: 10rpx;border-top-right-radius: 10rpx;}
.start-conent .start-player .start-player-head{position: absolute;top: -77rpx;width: 100%;}
.start-conent .start-player .img-palyer-head{width: 174rpx;height: 174rpx;border-radius: 50%;display: block;margin: 0 auto;}
.start-conent .start-player .start-player-name{width: 152rpx;height: 38rpx;line-height: 38rpx;padding: 6rpx 10rpx;font-size: 24rpx;text-align: center;border-radius: 40rpx;margin: -11rpx auto 0;background-color: #fff;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 1;overflow: hidden;}
.start-conent .start-player .start-Challenge-title{font-size: 22rpx;padding: 149rpx 0 25rpx;}
.start-conent .start-player .start-Challenge-score{font-size: 72rpx;font-weight: bold;padding-bottom: 54rpx;}
.start-conent .start-player .start-valid-time{font-size: 22rpx;padding-bottom: 48rpx;}
.start-conent .start-player .btn-Challenge{font-size: 24rpx;color: #fff;width: 170rpx;height: 61rpx;line-height: 61rpx;margin: 0 auto;border-radius: 50rpx;background-color: #F46A2E;margin-bottom: 30rpx;display: none;}
.start-conent .receive-player-list{background-color: #FFDE94;border-bottom-left-radius: 10rpx;border-bottom-right-radius: 10rpx;}

.start-conent .receive-player-list{height: 289rpx;background-color: #FFDE94;border-bottom-left-radius: 10rpx;border-bottom-right-radius: 10rpx;overflow-y: auto;}
.start-conent .receive-player-list .no-Challenge-player{width: 100%;height: 289rpx;line-height: 289rpx;text-align: center;font-size: 24rpx;color: #999;display: none;}
.start-conent .receive-player-list .challenge-result{margin-right: 22rpx;font-size: 24rpx;color: #DF6240;}
.start-conent .receive-player-list .challenge-player-item{display: flex;justify-content: space-between;align-items: center;border-bottom: 1rpx solid #F6CF67;padding: 18rpx 0;}
.start-conent .receive-player-list .challenge-my-item{background-color: #F4BF4E;}
.start-conent .receive-player-list .challenge-player-flex{display: flex;align-items: center;}
.start-conent .receive-player-list .challenge-player-rank{width: 38rpx;height: 38rpx;line-height: 38rpx;text-align: center;border-radius: 50%;color: #fff;background-color: #FACC3F;margin-left: 29rpx;}
.start-conent .receive-player-list .challenge-player-head{width: 60rpx;height: 60rpx;display: block;border-radius: 50%;margin-left: 32rpx;}
.start-conent .receive-player-list .challenge-player-mark{font-size: 24rpx;color: #333;margin-left: 21rpx;}
.start-conent .receive-player-list .challenge-player-i{line-height: 40rpx;}

.direct-play{text-align: center;color: #FFFFFF;font-size: 30rpx;padding: 70rpx 0 134rpx;}
.img-adv{height: 155rpx;width: 100%;}

